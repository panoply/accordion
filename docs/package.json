{
  "name": "relapse-docs",
  "private": true,
  "description": "Documentation and example for Relapse",
  "version": "0.2.0",
  "homepage": "https://github.com/panoply/accordion",
  "author": {
    "name": "Νίκος Σαβίδης",
    "email": "n.savvidis@gmx.com",
    "url": "https://brixtoltextiles.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/panoply/relapse.git"
  },
  "bugs": {
    "url": "https://github.com/panoply/relapse/issues"
  },
  "license": "MIT",
  "keywords": [
    "accordion",
    "component",
    "collapse",
    "toggle"
  ],
  "scripts": {
    "dev": "rm -rf public && pnpm run sass:build && concurrently \"pnpm run js:watch\" \"pnpm run sass:watch\" \"pnpm run 11ty:watch\"",
    "build": "rm -rf public && pnpm run js:build && pnpm run sass:build && pnpm run 11ty:build",
    "11ty:watch": "eleventy --serve --watch",
    "11ty:build": "eleventy",
    "js:build": "tsup --minify",
    "js:watch": "tsup --watch",
    "sass:build": "sass --load-path=node_modules ./src/assets/styles/stylesheet.scss public/style.css --no-source-map",
    "sass:watch": "sass --watch ./src/assets/styles/stylesheet.scss public/style.css --no-source-map --poll",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "browserslist": [
    "defaults",
    "Chrome >= 78",
    "Firefox >= 78",
    "Safari >= 10",
    "Edge >= 18",
    "IE >= 11",
    "Opera >= 73",
    "ChromeAndroid >= 75",
    "ios_saf >= 9",
    "Android >= 4",
    "not dead"
  ],
  "stylelint": {
    "extends": "@sissel/stylelint-config",
    "ignoreFiles": [
      "**/node_modules"
    ],
    "rules": {
      "property-no-vendor-prefix": null
    }
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@brixtol/bootstrap": "^1.7.1",
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@panoply/11ty": "^0.2.0",
    "@sardine/eleventy-plugin-tinyhtml": "^0.2.0",
    "@sissel/stylelint-config": "^1.1.0",
    "@types/markdown-it": "^13.0.5",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "cssnano": "^6.0.1",
    "esbuild": "^0.19.5",
    "markdown-it": "^13.0.2",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-container": "^3.0.0",
    "papyrus": "link:../../../liquify/packages/papyrus",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "sass": "^1.69.5",
    "stylelint": "^15.11.0",
    "tsup": "^7.2.0"
  },
  "dependencies": {
    "@hotwired/stimulus": "^3.2.2",
    "qvp": "^0.3.2",
    "relapse": "link:..",
    "spx": "0.1.2-beta.1"
  }
}